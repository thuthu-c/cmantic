program caso_com_chamada
begin
    procedure obter_bonus(): int
    begin
        var bonus := 100
    in
        return bonus
    end;

    procedure main()
    begin
        var nivel: int := 2;
        var bonus: int := 0
    in
        case nivel of
            1: bonus := 10 |
            2: bonus := 20 + obter_bonus() |
            3: bonus := 30
        esac
    end
end
